{
  "compilerOptions": {
    "jsx": "react-jsx",
    "allowJs": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "jsxImportSource": "@emotion/react",
    "types": ["vite/client", "vitest"],
    "paths": {
      "@promptsleuth/core": ["libs/core/src/index.ts"],
